# Supabase connection (server-side only, used by scripts)
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Supabase connection (available to client components)
# These must be duplicates of the above for Next.js client-side access
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Corpus data
CORPUS_REPO_PATH=./mckenna-corpus
